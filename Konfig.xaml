        <Button Content="Kék" Background="#00000000" HorizontalContentAlignment="Center" FontFamily="Gill Sans MT" FontSize="40" VerticalContentAlignment="Center" Padding="1" HorizontalAlignment="Center" VerticalAlignment="Center" BorderBrush="#00707070" RenderTransformOrigin="0.745,0.087" Canvas.Left="710" Canvas.Top="705" />
        <Button Content="Piros" Background="#00000000" HorizontalContentAlignment="Center" FontFamily="Gill Sans MT" FontSize="40" VerticalContentAlignment="Center" Padding="1" HorizontalAlignment="Center" VerticalAlignment="Center" BorderBrush="#00707070" RenderTransformOrigin="0.745,0.087" Canvas.Left="812" Canvas.Top="705" />
        <Button Content="Kerék 1" Background="#00000000" HorizontalContentAlignment="Center" FontFamily="Gill Sans MT" FontSize="40" VerticalContentAlignment="Center" Padding="1" HorizontalAlignment="Center" VerticalAlignment="Center" BorderBrush="#00707070" RenderTransformOrigin="0.745,0.087" Canvas.Left="350" Canvas.Top="875" />
