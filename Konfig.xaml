        <Button Content="Fekete belső" Background="#00000000" HorizontalContentAlignment="Center" FontFamily="Gill Sans MT" FontSize="40" VerticalContentAlignment="Center" Padding="1" HorizontalAlignment="Center" VerticalAlignment="Center" BorderBrush="#00707070" RenderTransformOrigin="0.745,0.087" Canvas.Left="1134" Canvas.Top="800" />
        <Button Content="Fekete-fehér belső" Background="#00000000" HorizontalContentAlignment="Center" FontFamily="Gill Sans MT" FontSize="40" VerticalContentAlignment="Center" Padding="1" HorizontalAlignment="Center" VerticalAlignment="Center" BorderBrush="#00707070" RenderTransformOrigin="0.745,0.087" Canvas.Left="1532" Canvas.Top="809" />

    </Canvas>
</UserControl>
